{
  "env": {
      "browser": true,
      "es6": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "react-hooks"
  ],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/prop-types": "off",
    "no-unused-vars":"warn"
  }
}
